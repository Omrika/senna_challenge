<div class="col-md-4 col-sm-6 col-xs-6" id="center">
  <div class="panel panel-default">
    <div class="panel-heading clearfix">
      <h3 class="panel-title">Find Follower Count</h3>
    </div>
    <div class="panel-body">
      <form class="form-horizontal row-border" action="#" id="instagram">
        <div class="form-group">
          <label class="col-md-3 control-label">Username</label>
          <div class="col-md-8">
            <input type="text" name="username" class="form-control">
          </div>
        </div>
        <label class="radio-inline">
          <input type="radio" name="social_network" id="optionsRadios1" value="twitter"> Twitter
        </label>
        <label class="radio-inline">
          <input type="radio" name="social_network" id="optionsRadios2" value="instagram"> Instagram
        </label>
        <div class="panel-body">
        <button type="submit" class="btn btn-info" value="Submit">Submit</button>
        </div>
        <!-- follower count-->
        <div id="container">
        </div>

      </form>
    </div>
  </div>
</div>
